    <body class="appear-animate">
        
        <!-- Page Loader -->        
        <div class="page-loader">
            <div class="loader">Loading...</div>
        </div>
        <!-- End Page Loader -->
        
        <!-- Page Wrap -->
        <div class="page" id="top">
            
            
            
            
            <!-- Section -->
            <section class="page-section" style="background:white">
                <div class="container relative">
                    
                    <div class="row">
                        
                        <!-- Content -->
                        <div class="col-sm-12">
                            
                            <!-- Shop options -->
                            <div class="clearfix mb-40">
                                
                                <div class="left section-text mt-10">
                                    Showing 1 â€“ <% if (@events.count >12) %>12 <% else %> <%= @events.count%><%end%> of <%= @events.count%> results
                                </div>
                                
                                <div class="right">
                                    <form method="post" action="#" class="form">
                                        <select class="input-md round">
                                            <option>Default sorting</option>
                                            <option>Sort by price: low to high</option>
                                            <option>Sort by price: high to low</option>
                                        </select>
                                    </form>
                                </div>
                                
                            </div>
                            <!-- End Shop options -->
                            
                            <div class="row multi-columns-row">
                                <% @events.each do |event| %>
                                <!-- Shop Item -->
                                <div class="col-md-3 col-lg-3 mb-60 mb-xs-40 item-event">
                                    
                                    <div class="post-prev-img">
                                        
                                        <%if (event.event_images.exists?)%>
                                         
                                            <%= link_to event ,:class => "event-link" do%><img src="<%=event.event_images.first.attachment.url%>" alt="" /><% end %>
                                        <%else%>
                                            <%= link_to event ,:class => "event-link" do%><img src="https://s3-us-west-2.amazonaws.com/stubfair-images/event_images/attachments/original/missing.png"  alt="" /><% end %>
                                        <%end%>
                                        
                                        <!-- <div class="intro-label">
                                            <span class="label label-danger bg-red">Sale</span>
                                        </div> -->
                                        
                                    </div>
                                    
                                    <div class="post-prev-title font-alt align-center">
                                        
                                        <%= link_to event ,:class => "event-link" do%><%= event.title %><% end %>
                                    </div>

                                    <div class="post-prev-text align-center">
                                        <%= link_to  controller:"tickets", action:"new", event_id: event.id do%><div class="call-to-action-sale sell">Sell Ticket</div><%end%>
                                        <%= link_to event ,:class => "event-link" do%><div class="call-to-action-sale buy">Buy Ticket</div>
                                        <%end%>
                                    </div>
                                    
                                    <!-- <div class="post-prev-more align-center">
                                        <a href="#" class="btn btn-mod btn-gray btn-round"><i class="fa fa-shopping-cart"></i> Add to cart</a>
                                    </div> -->
                                    
                                </div>
                                <!-- End Shop Item -->
                                <% end %>
                            </div>
                            
                            <!-- Pagination -->
                            <div class="pagination">
                                <a href=""><i class="fa fa-angle-left"></i></a>
                                <a href="" class="active">1</a>
                                <a href="">2</a>
                                <a href="">3</a>
                                <a class="no-active">...</a>
                                <a href="">9</a>
                                <a href=""><i class="fa fa-angle-right"></i></a>
                            </div>
                            <!-- End Pagination -->
                            
